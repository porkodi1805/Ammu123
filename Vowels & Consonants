def count_vowels_consonants(s: str) -> None:
    vowels = "aeiouAEIOU"
    v_count = 0
    c_count = 0
    
    for char in s:
        if char.isalpha():  
            if char in vowels:
                v_count += 1
            else:
                c_count += 1
    
    print(f"Vowels = {v_count}, Consonants = {c_count}")


count_vowels_consonants("hello world")  
count_vowels_consonants("Python")       
