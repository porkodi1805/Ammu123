def is_palindrome(s: str) -> None:
    
    s_clean = s.replace(" ", "").lower()
    
    if s_clean == s_clean[::-1]:
        print("Yes")
        print(f'"{s}" is a palindrome.')
    else:
        print("No")
        print(f'"{s}" is not a palindrome.')


is_palindrome("madam") 
is_palindrome("python")
